<div id="<%= dom_id order %>">
  <p>
    <strong>Title:</strong>
    <%= order.title %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= order.description %>
  </p>

  <p>
    <strong>Price:</strong>
    <%= order.price %>
  </p>

  <p>
    <strong>Quantity:</strong>
    <%= order.quantity %>
  </p>

  <p>
    <strong>Email address:</strong>
    <%= order.emailAddress %>
  </p>

</div>
